<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8" />
    <title>Stock Report</title>

    <style>
        body {
            font-family: Arial, sans-serif;
            font-size: 12px;
        }

        h1 {
            text-align: center;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 20px;
        }

        th,
        td {
            border: 1px solid #ccc;
            padding: 8px;
            text-align: left;
        }

        th {
            background-color: #f3f3f3;
        }
    </style>
</head>

<body>

    <h1>Stock Report</h1>

    <table>
        <thead>
            <tr>
                <th>SKU</th>
                <th>Product</th>
                <th>Qty</th>
                <th>Location</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach(item=> { %>
                <tr>
                    <td>
                        <%= item.sku %>
                    </td>
                    <td>
                        <%= item.name %>
                    </td>
                    <td>
                        <%= item.qty %>
                    </td>
                    <td>
                        <%= item.location %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>

</body>

</html>